{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Spring": {
    "Application": {
      "Name": "SERVICE.TRAVELPORTAL.SEARCH"
    }
  },
  "Eureka": {
    "Client": {
      "ServiceUrl": {
        "DefaultZone": "http://localhost:8761/eureka/"
      },
      "ValidateCertificates": false,
      "ShouldRegisterWithEureka": true
    },
    "Instance": {
      "NonSecurePort": 24539,
      "HostName": "localhost",
      "InstanceId": "Search.TRAVELPORTAL,Port:24539",
      "HealthCheckUrlPath": "/api/v1/HealthCheck",
      "StatusPageUrlPath": "/api/values/status"
    }
  }
}
