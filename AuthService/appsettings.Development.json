{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Spring": {
    "Application": {
      "Name": "SERVICE.TRAVELPORTAL.AUTH"
    }
  },
  "Eureka": {
    "Client": {
      "ServiceUrl": {
        "DefaultZone": "http://localhost:8761/eureka/"
      },
      "ValidateCertificates": false,
      "ShouldRegisterWithEureka": true
    },
    "Instance": {
      "NonSecurePort": 34644,
      "HostName": "localhost",
      "InstanceId": "Auth.TRAVELPORTAL,Port:34644",
      "HealthCheckUrlPath": "/api/v1/HealthCheck",
      "StatusPageUrlPath": "/api/values/status"
    }
  },
  "JWT": {
    "Key": "This is my interesting secret key for jwt"
  }
}
